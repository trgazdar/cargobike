<odoo>
	<data noupdate="1" >
		<act_window
			id           ="export_odoo_product_template_action"
			name         ="Export/Update Templates"
			binding_model="product.template"
			res_model    ="export.templates"
			view_mode    ="form"
			target       ="new"
		/>

		<act_window
			id           ="export_odoo_products_action"
			name         ="Export/Update Products"
			binding_model="product.product"
			res_model    ="export.products"
			view_mode    ="form"
			target       ="new"
		/>

		<act_window
			id           ="export_odoo_products_category"
			name         ="Export/Update Categories"
			binding_model="product.category"
			res_model    ="export.categories"
			view_mode    ="form"
			target       ="new"
		/>

